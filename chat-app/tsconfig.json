{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "jsx": "react-jsx",
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "lib": ["ESNext", "DOM"],
    "ignoreDeprecations": "6.0",
    "baseUrl":".",
    "paths": {
      "@/*":["./*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
